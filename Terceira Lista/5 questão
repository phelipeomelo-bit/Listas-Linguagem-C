#include <stdio.h>

int main() {
    int vetor[10];
    int numero, encontrado = 0;

    printf("Digite 10 números inteiros:\n");
    for (int i = 0; i < 10; i++) {
        printf("Número %d: ", i + 1);
        scanf("%d", &vetor[i]);
    }

    printf("Digite o número que deseja buscar: ");
    scanf("%d", &numero);

    for (int i = 0; i < 10; i++) {
        if (vetor[i] == numero) {
            encontrado = 1;
            break;
        }
    }

    if (encontrado) {
        printf("O número %d está presente no vetor.\n", numero);
    } else {
        printf("O número %d NÃO está presente no vetor.\n", numero);
    }

    return 0;
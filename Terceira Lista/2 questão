#include <stdio.h>

int main() {
    int vetor[15];
    int maior, menor;

    printf("Digite 15 números inteiros:\n");
    for (int i = 0; i < 15; i++) {
        printf("Número %d: ", i + 1);
        scanf("%d", &vetor[i]);
        if (i == 0) {
            maior = menor = vetor[i];
        } else {
            if (vetor[i] > maior) {
                maior = vetor[i];
            }
            if (vetor[i] < menor) {
                menor = vetor[i];
            }
        }
    }

    printf("Maior valor: %d\n", maior);
    printf("Menor valor: %d\n", menor);

    return 0;
} 
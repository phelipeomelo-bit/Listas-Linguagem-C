#include <stdio.h>

int main() {
    int n, a = 0, b = 1, proximo;

    printf("Quantos elementos da sequência de Fibonacci deseja exibir? ");
    scanf("%d", &n);

    if (n <= 0) {
        printf("Quantidade inválida.\n");
        return 0;
    }

    printf("Sequência de Fibonacci:\n");
    for (int i = 0; i < n; i++) {
        if (i == 0) {
            printf("%d ", a);
        } else if (i == 1) {
            printf("%d ", b);
        } else {
            proximo = a + b;
            printf("%d ", proximo);
            a = b;
            b = proximo;
        }
    }
    printf("\n");

    return 0;
}